{
    "name": "nolanle/sms-notification",
    "description": "SMS notification for Laravel framework.",
    "keywords": ["laravel", "sms-notificaion", "esms"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/nolanle/sms-notification/issues",
        "source": "https://github.com/nolanle/sms-notification"
    },
    "authors": [
        {
            "name": "Nolan Le",
            "email": "hoanglek13@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/support": "~5.6"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "NolanLe\\SmsNotification\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "NolanLe\\SmsNotification\\SmsNotificationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}